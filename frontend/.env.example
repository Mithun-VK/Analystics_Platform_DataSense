# .env.example
# Environment Configuration Template
# Copy this file to .env and fill in the values for your environment

# ============================================================================
# Application Settings
# ============================================================================
VITE_APP_NAME=DataAnalytics
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development
VITE_APP_DEBUG=true

# ============================================================================
# API Configuration
# ============================================================================
# Backend API base URL
VITE_API_URL=http://localhost:3000/api

# Request timeout in milliseconds
VITE_API_TIMEOUT=30000

# Number of retry attempts for failed requests
VITE_API_RETRY_ATTEMPTS=3

# ============================================================================
# Authentication
# ============================================================================
# Local storage keys for auth data
VITE_AUTH_TOKEN_KEY=auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=auth_refresh_token
VITE_AUTH_USER_KEY=auth_user

# Session timeout in milliseconds (1800000 = 30 minutes)
VITE_SESSION_TIMEOUT=1800000

# ============================================================================
# Social Authentication
# ============================================================================
# OAuth Provider Client IDs
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GITHUB_CLIENT_ID=your_github_client_id_here
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here

# ============================================================================
# File Upload
# ============================================================================
# Maximum file size in bytes (524288000 = 500 MB)
VITE_MAX_FILE_SIZE=524288000

# Maximum number of files to upload at once
VITE_MAX_UPLOAD_FILES=10

# Allowed file extensions (comma-separated)
VITE_ALLOWED_FILE_TYPES=.csv,.xlsx,.xls,.json,.parquet

# ============================================================================
# Analytics & Monitoring
# ============================================================================
# Enable/disable analytics
VITE_ENABLE_ANALYTICS=true

# Analytics service API key
VITE_ANALYTICS_KEY=your_analytics_key_here

# Sentry error tracking DSN
VITE_SENTRY_DSN=your_sentry_dsn_here

# ============================================================================
# Feature Flags
# ============================================================================
# Enable multi-factor authentication
VITE_ENABLE_MFA=true

# Enable social authentication
VITE_ENABLE_SOCIAL_AUTH=true

# Enable AI insights
VITE_ENABLE_INSIGHTS=true

# Enable data export functionality
VITE_ENABLE_EXPORT=true

# Enable data cleaning tools
VITE_ENABLE_CLEANING=true

# Enable collaboration features
VITE_ENABLE_COLLABORATION=true

# ============================================================================
# Storage & Caching
# ============================================================================
# Storage type: localStorage or sessionStorage
VITE_STORAGE_TYPE=localStorage

# Cache duration in milliseconds (900000 = 15 minutes)
VITE_CACHE_DURATION=900000

# ============================================================================
# Logging & Debugging
# ============================================================================
# Log level: debug, info, warn, error
VITE_LOG_LEVEL=debug

# Enable request logging
VITE_ENABLE_REQUEST_LOGGING=true

# Enable error logging
VITE_ENABLE_ERROR_LOGGING=true

# ============================================================================
# Performance Monitoring
# ============================================================================
# Enable performance monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Performance threshold in milliseconds
VITE_PERFORMANCE_THRESHOLD=3000
