{
  "name": "dataanalytics-frontend",
  "version": "1.0.0",
  "description": "Advanced Data Analysis Platform - Frontend Application",
  "author": "Your Team",
  "license": "MIT",
  "homepage": "https://dataanalytics.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dataanalytics.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dataanalytics/issues",
    "email": "support@dataanalytics.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "data-analysis",
    "data-visualization",
    "eda",
    "charts",
    "analytics",
    "react",
    "typescript",
    "dashboard"
  ],
  "scripts": {
    "dev": "vite",
    "dev:inspect": "vite --inspect-brk --inspect",
    "build": "npm run type-check && npm run lint && vite build",
    "build:staging": "npm run type-check && vite build --mode staging",
    "build:production": "npm run type-check && vite build --mode production",
    "build:analyze": "npm run build && npm run analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --single-thread",
    "analyze": "vite-bundle-visualizer dist",
    "serve": "vite preview --port 5173",
    "start": "npm run dev",
    "clean": "rm -rf dist node_modules .turbo .next",
    "clean:build": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "dedupe": "npm dedupe",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update-deps": "npm update",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prepare": "husky install",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.25.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.294.0",
    "qs": "^6.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.0",
    "sonner": "^1.3.0",
    "tailwind-merge": "^2.2.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.12",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "yarn": ">=3.0.0",
    "pnpm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not dead",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  },
  "sideEffects": false,
  "packageManager": "npm@10.2.3"
}
